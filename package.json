{
  "name": "discord.ts-example",
  "version": "1.0.0",
  "description": "discordx bot example",
  "keywords": [
    "discordx",
    "discord.ts",
    "discord.js",
    "bot",
    "discord",
    "typescript"
  ],
  "homepage": "https://github.com/oceanroleplay/discord.ts-example#readme",
  "bugs": {
    "url": "https://github.com/oceanroleplay/discord.ts-example/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oceanroleplay/discord.ts-example.git"
  },
  "license": "MIT",
  "author": "Indian Ocean Roleplay",
  "contributors": [
    "Harry <indianoceanroleplay@gmail.com> (https://github.com/oceanroleplay)"
  ],
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
    "start": "nodemon --exec node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only src/main.ts",
    "serve": "node build/main.js"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.2",
    "@discordx/koa": "^1.0.7",
    "@discordx/pagination": "^2.0.1",
    "@discordx/utilities": "^3.4.2",
    "@koa/router": "^10.1.1",
    "@nervina-labs/flashsigner": "^0.2.2",
    "@nervosnetwork/ckb-sdk-rpc": "^0.102.1",
    "@nervosnetwork/ckb-sdk-utils": "^0.102.1",
    "discord.js": "^13.6.0",
    "discordx": "^9.2.5",
    "dotenv": "^16.0.0",
    "firebase-admin": "^11.5.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@discordx/changelog": "^2.0.7",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa__router": "^8.0.11",
    "@types/node": "^17.0.18",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
